// This file automatically generated by Puppet

options {
	directory "<%= @working_path %>";
<% unless @pid_file.nil? -%>
	pid-file "<%= @pid_file %>";
<% end -%>
<% unless @statistics_file.nil? -%>
	statistics-file "<%= @statistics_file %>";
<% end -%>
<% unless @dump_file.nil? -%>
	dump-file "<%= @dump_file %>";
<% end -%>
	querylog <%= @log_queries_by_default ? "yes" : "no" %>;
<% @options.each do |k,v| -%>
	<%= k %> <%= v %>;
<% end -%>
};
